#Identificar vocales

n1=str(input("Escriba una letra "))

if n1== "a":
  print ("Es vocal")
elif n1== "e":
  print ("Es vocal")
elif n1== "i":
  print ("Es vocal")
elif n1== "o":
  print ("Es vocal")
elif n1== "u":
  print ("Es vocal")

else:
  print("No es vocal")

if (len(n1)>1):
 print ("No se puede procesar el dato")
else:
  print ("Se puede procesar el dato")
