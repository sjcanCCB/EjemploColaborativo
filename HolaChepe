#Es un ejemplo de una calculadora

print ("Hola Chepe")
n1 = 52
n2 = 15

print ("Esto es una suma")
res = n1+n2
print (res)

print ("Esto es una resta")
res = n1-n2
print (res)

print ("Esto es una multiplicacion")
res = n1*n2
print (res)

print ("Esto es una division")
res = n1/n2
print (res)

print ("Esto es una modulo")
res = n1%n2
print (res)
